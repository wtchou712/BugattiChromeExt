{
    "name": "Instaritto",
    "description": "Select your favorite foods to instantly order and deliver with Postmates",
    "version": "0.1",
    "minimum_chrome_version": "16.0.884",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png"
    },
    "permissions": [
        "https://www.api.postmates.com",
        "http://www.api.postmates.com"
    ]
}